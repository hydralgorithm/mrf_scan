{
  "version": 2,
  "builds": [
    {
      "src": "app/dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/dashboard/$1"
    }
  ],
  "buildCommand": "cd app/dashboard && npm install && npm run build",
  "outputDirectory": "app/dashboard/dist",
  "installCommand": "cd app/dashboard && npm install"
}
